{
  "compilerOptions": {
    "target": "es2018",
    "module": "esnext",
    "moduleResolution": "node",
    "lib": [
      "esnext",
      "esnext.asynciterable",
      "dom"
    ],
    "esModuleInterop": true,
    "allowJs": true,
    "sourceMap": true,
    "strict": true,
    "noEmit": true,
    "baseUrl": ".",
    "paths": {
      "~/": ["./"],
      "~/*": ["./*"],
      "~~/*": ["./*"],
      "@": ["./"],
      "@/*": ["./*"],
      "@@/*": ["./*"]
    },
    "types": [
      "@types/node",
      "@nuxt/types",
      "@nuxtjs/axios",
      "./types"
    ]
  },
  "exclude": ["node_modules", ".nuxt", "dist"]
}